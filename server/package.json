{
  "name": "@moma/server",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "tsc": "tsc",
    "dev": "ts-node-dev --files --respawn src/index.ts",
    "build": "run-s clean tsc",
    "start": "node .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed --preview-feature",
    "prisma:dev": "run-s -l prisma:generate prisma:migrate prisma:seed",
    "prisma:reset": "prisma db push --accept-data-loss --schema=./prisma/reset.prisma; prisma db push --accept-data-loss"
  },
  "dependencies": {
    "@prisma/client": "3.7.0",
    "express": "^4.17.2",
    "express-promise-router": "^4.1.1",
    "express-session": "^1.17.2",
    "http-errors": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/http-errors": "^1.8.1",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "prisma": "3.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4",
    "yarn-run-all": "^3.1.1"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
